@model WantedPeoplePageListingModel

@{
    ViewData["Title"] = "Search results";
}

<h2>@ViewData["Title"]</h2>

@if (Model.WantedPeople.Any())
{
    <h3>Found @Model.SearchCriteriaTotalPages matched results.</h3>

    <div class="row">
        @await Html.PartialAsync("_WantedPeopleListingsPartial", Model)
    </div>
}
else
{
    <h3>No people to show.</h3>
}
